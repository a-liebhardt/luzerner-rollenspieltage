{{#with navigation}}
  <{{#if navigation-ol}}ol{{else}}ul{{/if}} role="{{#if navigation-role}}{{navigation-role}}{{^}}navigation{{/if}}" {{#if navigation-class}}class="{{navigation-class}}"{{/if}}>
    {{#each navigation-items}}
      <li class="{{#if navigation-current}}current{{/if}} {{#if navigation-active}}active{{/if}} {{navigation-class}}" {{{navigation-attr}}}>
        {{> @navlist-item}}
      </li>
    {{/each}}
  </{{#if navigation-ol}}ol{{else}}ul{{/if}}>
{{/with}}
